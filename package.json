  "homepage": "https://github.com/KudooCloud/kudoo-graphql.git",
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "apollo-link": "^1.2.3",
    "apollo-link-http": "1.5.7",
    "cross-fetch": "^2.2.2",
    "es6-promise": "^4.2.5",
    "exports-loader": "^0.7.0",
